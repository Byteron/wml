{
    "name": "wml",
    "displayName": "WML",
    "description": "Wesnoth Markup Language Syntax Highlightning",
    "publisher": "Bitron",
    "version": "1.0.1",
    "bugs": { "url": "https://github.com/Byteron/wml/issues"},
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "wml",
            "aliases": ["WML", "wml"],
            "extensions": [".cfg", ".pbl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "wml",
            "scopeName": "source.wml",
            "path": "./syntaxes/wml.tmLanguage.json"
        }]
    }
}